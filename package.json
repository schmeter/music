  "scripts": {
    "start": "grunt default",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint \"**/*.scss\"",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:fix": "npm run lint:css:fix && npm run lint:js:fix",
    "test": "jest",
    "test:watch": "npm test -- --watch -u",
    "test:cover": "npm test -- --coverage -u",
    "check": "npm run lint && npm test",
    "build": "grunt build:release && npm run check",
    "build:prod": "grunt build:prod && npm run check",
    "preview": "npm run build:prod && grunt browserSync watch",
    "update": "npm-check-updates -u && npm i"
  },
